<?xml version="1.0" encoding="utf-8"?>


<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="21" minHeight="21" 
             alpha.disabled="0.5"
			 >
    
    <!-- states -->
    <s:states>
        <s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:RadialGradient>
				<s:GradientEntry color="#ffffff" color.up="#000000" alpha=".05" alpha.over=".15" alpha.down=".25"/>
				<s:GradientEntry ratio=".4" color="#ffffff" color.up="#000000" alpha="0.02"/>
				<s:GradientEntry color="#ffffff" color.up="#000000" alpha="0.0"/>
			</s:RadialGradient>
		</s:fill>
	</s:Rect>
	
	
	<s:BitmapImage id="iconDisplay" horizontalCenter="0" verticalCenter="0" />
</s:SparkSkin>

